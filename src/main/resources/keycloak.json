{
  "realm": "kc-auth-realm",
  "auth-server-url": "http://auth-service:8080/",
  "ssl-required": "external",
  "resource": "ks-auth-client",
  "public-client": true,
  "confidential-port": 0,
  "principal-attribute": "name",
  "bearer-only": true,
  "enable-cors": true,
  "use-resource-role-mappings": false,
  "verify-token-audience": false,
  "realm-public-key": ""
}